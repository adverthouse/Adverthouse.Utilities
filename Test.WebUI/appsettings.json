{ 
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppSettings": {
    "RedisConfig": {
      "Enabled": "true",
      "RedisHost": "127.0.0.1",
      "RedisPort": "6379",
      "DefaultCacheTime": "15",
      "EnableReCache": "true",
      "DefaultReCacheTime": "5"
    }
  }
}
